{
  "name": "ide-scala",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "Scala language support for Atom IDE using Scalameta language server",
  "author": {
    "name": "Alexey Alekhin",
    "email": "laughedelic@gmail.com",
    "url": "https://github.com/laughedelic"
  },
  "keywords": [
    "scala",
    "scalameta",
    "ide",
    "atom-ide",
    "language-server",
    "language-server-protocol"
  ],
  "repository": "https://github.com/laughedelic/atom-ide-scala",
  "bugs": "https://github.com/laughedelic/atom-ide-scala/issues",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "enhancedScopes": [
    "source.scala"
  ],
  "dependencies": {
    "atom-languageclient": "0.6.7"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "provideHyperclick"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
